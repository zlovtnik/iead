		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-eslint-parser": "^1.3.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.4"
	}
